#Exercise for grade calculation for function



def computegrade(score):

  try:
    score=float(score)
  except:
    return 'Bad Score'

  if score>1 or score<0 :
    return 'Bad Score'
  elif score>= 0.9:
    return 'A'
  elif score>= 0.8:
    return 'B'
  elif score>= 0.7:
    return 'C'
  elif score>= 0.6:
    return 'D'
  else :
    return 'F'

score=input('Enter Score:')
computegrade(score)
